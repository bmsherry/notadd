{
    "name": "notadd-appliaction-starter",
    "version": "1.0.0",
    "description": "The starter application for notadd.",
    "scripts": {
        "start": "ts-node -r tsconfig-paths/register src/main.ts",
        "start:watch": "nodemon",
        "debug": "node --inspect-brk -r ts-node/register src/main.ts",
        "debug:watch": "nodemon --config nodemon-debug.json",
        "check": "tslint -p tsconfig.json -c tslint.json",
        "fix": "tslint -p tsconfig.json -c tslint.json --fix"
    },
    "author": "notadd",
    "license": "Apache-2.0",
    "private": false,
    "dependencies": {
        "@nestjs/common": "^5.1.0",
        "@nestjs/core": "^5.1.0",
        "@nestjs/graphql": "^3.0.0",
        "@nestjs/typeorm": "^5.1.0",
        "body-parser": "^1.18.3",
        "fastify": "^1.9.0",
        "fastify-formbody": "^2.0.1",
        "fastify-graphql-middleware": "^2.1.1",
        "graphql": "^0.13.2",
        "graphql-tools": "^3.0.5",
        "reflect-metadata": "^0.1.12",
        "rxjs": "^6.2.2",
        "typeorm": "^0.2.7",
        "typescript": "^2.9.2"
    },
    "devDependencies": {
        "@types/body-parser": "^1.17.0",
        "@types/graphql": "^0.13.3",
        "@types/node": "^10.5.2",
        "fastify-static": "^0.14.0",
        "handlebars": "^4.0.11",
        "nodemon": "^1.18.3",
        "point-of-view": "^1.3.0",
        "ts-node": "^7.0.0",
        "tsconfig-paths": "^3.4.2",
        "tslint": "^5.11.0"
